{
    "name": "rajabit/laravel-inventory",
    "description": "Laravel inventory",
    "keywords": [
        "laravel",
        "inventory"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/96rajabi",
        "source": "https://github.com/96rajabi/laravel-inventory"
    },
    "authors": [
        {
            "name": "Mehdi Rajabi",
            "email": "96rajabi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Rajabit\\LaravelInventory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rajabit\\LaravelInventory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "Rajabit\\LaravelInventory\\LaravelInventoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}